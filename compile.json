{
	"$schema": "https://raw.githubusercontent.com/qooxdoo/qooxdoo-compiler/master/resource/schema/v1/compile.json",
	"environment": {
		"qx.allowUrlVariants": true,
		"qx.allowUrlSettings": true,
		"qx.contrib": false,
		"qx.icontheme": ["Tango", "Oxygen"]
	},
	"applications": [{
		"class": "qxl.demobrowser.Application",
		"theme": "qxl.demobrowser.Theme",
		"name": "demobrowser",
		"title": "DemoBrowser",
		"include": [
			"qxl.demobrowser.*",
			"qx.theme.Indigo",
			"qx.theme.Modern",
			"qx.theme.Simple",
			"qx.theme.Classic"
		]
	}],
	/** Targets */
	"targets": [{
			"type": "source",
			"outputPath": "compiled/source"
		},
		{
			"type": "build",
			"outputPath": "compiled/build"
		}
	],

	"defaultTarget": "source",

	"locales": [
		"en"
	],
	"serve": {
		"listenPort": 8080
	}
}