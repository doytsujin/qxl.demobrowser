{
   "environment": {
    "qx.allowUrlVariants" : true,
	"qx.allowUrlSettings" : true,
	"qx.contrib": true,
    "qx.icontheme": ["Tango", "Oxygen"]
   },
	"applications": [
		{
			"class": "qxl.demobrowser.Application",
			"theme": "qxl.demobrowser.Theme",
			"name": "demobrowser",
			"title": "DemoBrowser",
			"include": [
				"qxl.demobrowser.*",
				"qx.theme.Modern",
				"qx.theme.Simple",
				"qx.theme.Classic"
			]
		}
	],
	/** Targets */
	"targets": [
		{
			"type": "source",
			"outputPath": "source-output"
		},
		{
			"type": "build",
			"outputPath": "build-output"
		}
	],
	
	"defaultTarget": "build",
	
	"locales": [
		"en"
	],
	"serve": {
	   "listenPort": 8080
	}
}