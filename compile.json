{
   "environment": {
    "qx.allowUrlVariants" : true,
	"qx.allowUrlSettings" : true,
	"qx.contrib": false,
    "qx.icontheme": ["Tango", "Oxygen"]
   },
	"applications": [
		{
			"class": "qxl.demobrowser.Application",
			"theme": "qxl.demobrowser.Theme",
			"name": "demobrowser",
			"title": "DemoBrowser",
			"default": false
		}
	],
    "include": [
		"qxl.demobrowser.*"
    ],
	/** Targets */
	"targets": [
		{
			"type": "source",
			"outputPath": "source-output"
		},
		{
			"type": "build",
			"outputPath": "build-output"
		}
	],
	
	"defaultTarget": "build",
	
	"locales": [
		"en"
	],
	"serve": {
	   "listenPort": 8080
	}
}